{
  "name": "across-tabs",
  "version": "0.1.0",
  "description": "Easily communicate among browser tabs(supports cross-origin)",
  "main": "dist/across-tabs.js",
  "scripts": {
    "build": "webpack --mode=build",
    "build-dev": "webpack-dashboard -- webpack --mode=dev",
    "build-test": "karma start --single-run --no-auto-watch karma.config.js",
    "dev": "webpack-dashboard -- webpack --progress --colors --watch --mode=dev",
    "test": "karma start karma.config.js",
    "coverage": "node node_modules/.bin/istanbul cover karma start --single-run --no-auto-watch karma.config.js",
    "coveralls": "cat ./coverage/report-lcov/lcov.info | node node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "5.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "coveralls": "^2.11.16",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "vue": "^2.0.3",
    "webpack": "1.12.9",
    "webpack-dashboard": "^0.3.0",
    "yargs": "3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wingify/across-tabs.git"
  },
  "keywords": [
    "across-tabs",
    "postmessage",
    "webpack",
    "es6",
    "umd",
    "commonjs"
  ],
  "author": "Varun Malhotra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingify/across-tabs/issues"
  },
  "homepage": "https://github.com/wingify/across-tabs",
  "dependencies": {}
}
